{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "../contracts/build/contracts/VoterChainElectionContract.json",
      "../contracts/build/contracts/VoterChainElectionsTrackerContract.json"
    ]
  },
  "rules": {
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [true, "single"]
  }
}
